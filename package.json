{
  "name": "localboast",
  "version": "0.0.12",
  "license": "MIT",
  "author": "Conor Kelleher - localboast@outlook.com",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conorkelleher/localboast.git"
  },
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "vite",
    "tsc": "tsc",
    "copy-files-to-build": "cp {package.json,README.md,LICENSE,.npmignore} build",
    "build": "tsc --project ./tsconfig.build.json && vite build && yarn copy-files-to-build",
    "dry-run": "cd build && npm publish --dry-run",
    "deploy": "cd build && npm publish",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "analyze": "npx vite-bundle-visualizer --output \"bundle_analysis.html\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "readmes": "tsx --loader esm-loader-css --loader esm-loader-images  --experimental-import-meta-resolve ./scripts/generateReadmes.ts",
    "build-storybook": "storybook build -o ../localboast.com/assets/storybook-static",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@jest/globals": "^29.7.0",
    "@mantine/core": "^7.1.5",
    "@mantine/hooks": "^7.1.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-controls": "^7.6.12",
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addons": "^7.6.12",
    "@storybook/blocks": "^7.6.12",
    "@storybook/react": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.6.12",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/highlight.js": "^10.1.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-jest": "^29.7.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "esm-loader-css": "^1.0.6",
    "esm-loader-images": "^1.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.9.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.12",
    "storybook-addon-deep-controls": "^0.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "yarn@4.0.0"
}
